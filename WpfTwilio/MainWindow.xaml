<Window x:Class="WpfTwilio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTwilio"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        xmlns:views="clr-namespace:WpfTwilio.View">
    <DockPanel x:Name="pnlPrincipal" LastChildFill="True">
        <Grid>

            <views:ContatosListaView Margin="0,0,5,0" />

            <views:EnviarMensagemView Grid.Column="1"/>

        </Grid>
    </DockPanel>
</Window>
